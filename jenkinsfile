pipeline {
    agent any
    environment{
        DOCKER = credentials('Docker')
    }
    stages {
        stage('Build'){
            steps {
                sh 'docker build -t msalcedo1/flask'
            }
        }stage('login'){
            steps {
                echo '$DOCKER' | docker login -u msalcedo1 --password-stdin'
            }
        }
    }
}